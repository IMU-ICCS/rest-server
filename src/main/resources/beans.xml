<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


  <bean id="userService" class="io.github.cloudiator.rest.UserServiceImpl">
  </bean>

  <bean id="cloudService" class="org.cloudiator.messaging.services.CloudServiceImpl">
  </bean>

  <bean id="messageInterface" class="org.cloudiator.messaging.kafka.KafkaMessageInterface">
  </bean>

  <bean id="kafkaFactory" class="io.github.cloudiator.KafkaFactory">
  </bean>

  <bean id="kafka" factory-bean="kafkaFactory" factory-method="createKafka"/>


</beans>
